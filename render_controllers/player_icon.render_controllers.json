{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.icon": {
      "geometry": "Geometry.icon",
      "materials": [ { "*": "Material.nametags" } ],
      "textures": [ "Texture.esp" ],
      "ignore_lighting": true,
      "overlay_color": {
        "r": "math.lerp(0,1,math.cos(query.life_time * v.color_speed))",
        "g": "math.lerp(0,1,-math.cos(query.life_time * v.color_speed))",
        "b": "math.lerp(0,1,math.sin(query.life_time * v.color_speed))",
        "a": "0.37"
      }
    }
  }
}
